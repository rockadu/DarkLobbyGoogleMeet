{
    "manifest_version": 3,
    "name": "Alterar Fundo do Google Meet",
    "version": "1.0",
    "description": "Change the background color of the Google Meet Lobby to black, to avoid burn your eyes.",
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "run_at": "document_end",
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": []
}